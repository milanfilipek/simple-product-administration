{
	"name": "milanfilipek/simple-product-administration",
	"description": "Simple product administration application built with Nette Framework",
	"keywords": ["nette"],
	"type": "project",
	"require": {
		"php": ">= 8.1",
		"nette/application": "^3.2.3",
		"nette/assets": "^1.0.0",
		"nette/bootstrap": "^3.2.6",
		"nette/caching": "^3.2",
		"nette/database": "^3.2",
		"nette/di": "^3.2",
		"nette/forms": "^3.2",
		"nette/http": "^3.3",
		"nette/mail": "^4.0",
		"nette/robot-loader": "^4.0",
		"nette/security": "^3.2",
		"nette/utils": "^4.0",
		"latte/latte": "^3.0",
		"tracy/tracy": "^2.10",
		"nextras/orm": "^5.0",
		"nextras/migrations": "^3.4",
		"nextras/dbal": "^5.0",
		"contributte/console": "^0.10"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"nette/tester": "^2.5",
		"phpstan/phpstan-nette": "^2"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
		}
	},
	"scripts": {
		"phpstan": "phpstan analyse",
		"tester": "tester tests -s"
	},
	"minimum-stability": "stable",
	"config": {
		"allow-plugins": {
			"symfony/thanks": false
		}
	}
}
